<div class="starter-template">
    <p class="lead" style="font-size: 2em">Lets Tweet</p>
</div>

<div class="col-md-6 container-fluid text-center">
    <form id="sendTweet">
        <input type="text" class="form-control" id="tweet">
        <br>
        <button class="btn btn-primary">Tweet</button>
    </form>
    <br>
    <br>

    <div id="tweets">
        <div class="media">
            <div class="media-left">
                <a href="#"><img class="media-object" src="{{ user.photo }}" /> </a>
            </div>
            <div class="media-body">
                <h4 class="media-heading">{{ user.name }}</h4>
                <p>Hello We Did It</p>
            </div>
        </div>
    </div>
</div>